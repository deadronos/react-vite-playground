    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "less": "^4.4.2",
    "postcss": "^8.5.6",
